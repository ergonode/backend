            "Ergonode\\AttributeDate\\": "module/attribute-date/src",
            "Ergonode\\AttributeImage\\": "module/attribute-image/src",
            "Ergonode\\Segment\\": "module/segment/src",
            "Ergonode\\Grid\\": "module/grid/src",
            "Ergonode\\Channel\\": "module/channel/src",
            "Ergonode\\Connector\\": "module/connector/src",
            "Ergonode\\Value\\": "module/value/src",
            "Ergonode\\ConnectorMagento\\": "module/connector-magento/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Ergonode\\Migration\\Tests\\": "module/migration/src",
            "Ergonode\\Account\\Tests\\": "module/account/tests",
            "Ergonode\\Authentication\\Tests\\": "module/authentication/tests",
            "Ergonode\\Core\\Tests\\": "module/core/tests",
