        "sensio/framework-extra-bundle": "^5.3",
        "symfony/asset": "^4.2",
        "symfony/cache": "^4.2",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/expression-language": "^4.2",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/lts": "^4@dev",
        "symfony/messenger": "^4.2",
        "symfony/monolog-bundle": "^3.3",
