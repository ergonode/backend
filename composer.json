{
    "name": "ergonode/backend",
    "description": "Ergonode - development backend package",
    "type": "project",
    "license": "OSL-3.0",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "beberlei/assert": "2.x",
        "doctrine/annotations": "^1.8",
        "doctrine/dbal": "^2.10.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/migrations": "2.2.x",
        "egulias/email-validator": "^2.1",
        "ergonode/exporter-shopware-6": "^1.0",
        "ergonode/importer-magento-1": "^1.0",
        "gesdinet/jwt-refresh-token-bundle": "^0.11.1",
        "guzzlehttp/guzzle": "^6.3",
        "league/csv": "^9.6",
        "league/flysystem": ">1.0.54",
        "league/flysystem-bundle": "^1.5",
        "lexik/jwt-authentication-bundle": "^2.6",
        "limenius/liform-bundle": "^0.15.0",
        "moneyphp/money": "^3.2",
        "nelmio/api-doc-bundle": "^3.6",
        "nelmio/cors-bundle": "^2.0",
        "phing/phing": "^2.16",
        "ramsey/collection": "^1.1",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "^4.4",
        "symfony/cache": "^4.4",
