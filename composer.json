        "symfony/expression-language": "^4.4",
        "symfony/flex": "^1.10",
        "symfony/form": "^4.4.11",
        "symfony/framework-bundle": "^4.4",
        "symfony/http-foundation": "^4.4",
        "symfony/mailer": "^4.4",
        "symfony/messenger": "^4.4",
        "symfony/monolog-bundle": "^3.5",
        "symfony/options-resolver": "^4.4",
        "symfony/polyfill-php80": "^1.0",
        "symfony/security": "^4.4",
        "symfony/security-bundle": "^4.4.16",
        "symfony/serializer": "^4.4",
        "symfony/stopwatch": "^4.4",
        "symfony/translation": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "^4.4.11",
        "symfony/yaml": "^4.4",
        "vich/uploader-bundle": "^1.10",
        "webmozart/assert": "^1.7",
        "zircote/swagger-php": "2.0.10"
    },
    "require-dev": {
        "behat/behat": "^3.6",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/symfony2-extension": "^2.1.5",
        "behatch/contexts": "^3.3",
        "escapestudios/symfony2-coding-standard": "^3.10",
        "friends-of-behat/symfony-extension": "^2.0.5",
        "infection/infection": "^0.17.5",
        "malukenho/docheader": "^0.1.8",
        "nelmio/alice": "^3.6",
        "phpstan/phpstan": "^0.12.42",
        "phpunit/phpunit": "^9.0",
        "qossmic/deptrac-shim": "^0.11.1",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/maker-bundle": "^1.13",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^4.4"
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Ergonode\\Account\\": "module/account/src",
            "Ergonode\\Api\\": "module/api/src",
            "Ergonode\\Attribute\\": "module/attribute/src",
            "Ergonode\\Authentication\\": "module/authentication/src",
            "Ergonode\\BatchAction\\": "module/batch-action/src",
            "Ergonode\\Category\\": "module/category/src",
            "Ergonode\\Channel\\": "module/channel/src",
            "Ergonode\\Comment\\": "module/comment/src",
            "Ergonode\\Completeness\\": "module/completeness/src",
            "Ergonode\\Condition\\": "module/condition/src",
            "Ergonode\\Core\\": "module/core/src",
            "Ergonode\\Designer\\": "module/designer/src",
            "Ergonode\\EventSourcing\\": "module/event-sourcing/src",
            "Ergonode\\ExporterFile\\": "module/exporter-file/src",
            "Ergonode\\Fixture\\": "module/fixture/src",
            "Ergonode\\Grid\\": "module/grid/src",
            "Ergonode\\Importer\\": "module/importer/src",
            "Ergonode\\ImporterErgonode1\\": "module/importer-ergonode-1/src",
            "Ergonode\\Mailer\\": "module/mailer/src",
            "Ergonode\\Migration\\": "module/migration/src",
            "Ergonode\\Multimedia\\": "module/multimedia/src",
            "Ergonode\\NewRelic\\": "module/newrelic/src",
            "Ergonode\\Notification\\": "module/notification/src",
            "Ergonode\\Product\\": "module/product/src",
            "Ergonode\\ProductCollection\\": "module/product-collection/src",
            "Ergonode\\Reader\\": "module/reader/src",
            "Ergonode\\Segment\\": "module/segment/src",
            "Ergonode\\SharedKernel\\": "module/shared-kernel/src",
            "Ergonode\\Value\\": "module/value/src",
            "Ergonode\\Workflow\\": "module/workflow/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Ergonode\\Account\\Tests\\": "module/account/tests",
            "Ergonode\\Api\\Tests\\": "module/api/tests",
            "Ergonode\\Attribute\\Tests\\": "module/attribute/tests",
            "Ergonode\\Authentication\\Tests\\": "module/authentication/tests",
            "Ergonode\\BatchAction\\Tests\\": "module/batch-action/tests",
            "Ergonode\\Category\\Tests\\": "module/category/tests",
            "Ergonode\\Channel\\Tests\\": "module/channel/tests",
            "Ergonode\\Comment\\Tests\\": "module/comment/tests",
            "Ergonode\\Completeness\\Tests\\": "module/completeness/tests",
            "Ergonode\\Condition\\Tests\\": "module/condition/tests",
            "Ergonode\\Core\\Tests\\": "module/core/tests",
            "Ergonode\\Designer\\Tests\\": "module/designer/tests",
            "Ergonode\\EventSourcing\\Tests\\": "module/event-sourcing/tests",
            "Ergonode\\ExporterFile\\Tests\\": "module/exporter-file/tests",
            "Ergonode\\Fixture\\Tests\\": "module/fixture/tests",
            "Ergonode\\Grid\\Tests\\": "module/grid/tests",
            "Ergonode\\Importer\\Tests\\": "module/importer/tests",
            "Ergonode\\ImporterErgonode1\\Tests\\": "module/importer-ergonode-1/tests",
            "Ergonode\\Mailer\\Tests\\": "module/mailer/tests",
            "Ergonode\\Migration\\Tests\\": "module/migration/src",
            "Ergonode\\Multimedia\\Tests\\": "module/multimedia/tests",
            "Ergonode\\NewRelic\\Tests\\": "module/newrelic/tests",
            "Ergonode\\Notification\\Tests\\": "module/notification/tests",
            "Ergonode\\Product\\Tests\\": "module/product/tests",
            "Ergonode\\ProductCollection\\Tests\\": "module/product-collection/tests",
            "Ergonode\\Reader\\Tests\\": "module/reader/tests",
            "Ergonode\\Segment\\Tests\\": "module/segment/tests",
            "Ergonode\\SharedKernel\\Tests\\": "module/shared-kernel/tests",
            "Ergonode\\Value\\Tests\\": "module/value/tests",
            "Ergonode\\Workflow\\Tests\\": "module/workflow/tests"
        }
    },
    "replace": {
        "ergonode/account": "self.version",
        "ergonode/api": "self.version",
        "ergonode/attribute": "self.version",
        "ergonode/authentication": "self.version",
        "ergonode/batch-action": "self.version",
        "ergonode/category": "self.version",
        "ergonode/channel": "1.0",
        "ergonode/comment": "self.version",
        "ergonode/completeness": "self.version",
        "ergonode/condition": "self.version",
        "ergonode/core": "self.version",
        "ergonode/designer": "self.version",
        "ergonode/event-sourcing": "self.version",
        "ergonode/exporter-file": "self.version",
        "ergonode/fixture": "self.version",
        "ergonode/grid": "self.version",
        "ergonode/importer": "1.0",
        "ergonode/importer-ergonode-1": "self.version",
        "ergonode/mailer": "self.version",
        "ergonode/migration": "self.version",
        "ergonode/multimedia": "self.version",
        "ergonode/newrelic": "self.version",
        "ergonode/notification": "self.version",
        "ergonode/product": "self.version",
        "ergonode/product-collection": "self.version",
        "ergonode/reader": "self.version",
        "ergonode/segment": "self.version",
        "ergonode/shared-kernel": "self.version",
        "ergonode/value": "self.version",
        "ergonode/workflow": "self.version",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "behat": [
            "php -d error_reporting='E_ALL & ~E_DEPRECATED' ./bin/behat --colors --stop-on-failure"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "doctrine/persistence": "<1.3"
    }
}
