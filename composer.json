            "Ergonode\\Multimedia\\Tests\\": "module/multimedia/tests",
            "Ergonode\\Fixture\\Tests\\": "module/fixture/tests",
            "Ergonode\\Designer\\Tests\\": "module/designer/tests",
            "Ergonode\\Editor\\Tests\\": "module/editor/tests",
            "Ergonode\\Configuration\\Tests\\": "module/configuration/tests",
            "Ergonode\\Importer\\Tests\\": "module/importer/tests",
            "Ergonode\\Reader\\Tests\\": "module/reader/tests",
            "Ergonode\\Transformer\\Tests\\": "module/transformer/tests",
            "Ergonode\\Category\\Tests\\": "module/category/tests",
            "Ergonode\\CategoryTree\\Tests\\": "module/category-tree/tests",
            "Ergonode\\Product\\Tests\\": "module/product/tests",
            "Ergonode\\ProductSimple\\Tests\\": "module/product-simple/tests",
            "Ergonode\\Completeness\\Tests\\": "module/completeness/tests",
            "Ergonode\\Navigation\\Tests\\": "module/navigation/tests",
            "Ergonode\\Attribute\\Tests\\": "module/attribute/tests",
            "Ergonode\\AttributePrice\\Tests\\": "module/attribute-price/tests",
            "Ergonode\\AttributeUnit\\Tests\\": "module/attribute-unit/tests",
