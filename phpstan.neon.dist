parameters:
    level: 2
    excludes_analyse:
        - %currentWorkingDirectory%/module/*/migrations/*
        - %currentWorkingDirectory%/module/*/tests/*
        - %currentWorkingDirectory%/module/core/src/Infrastructure\JMS\Serializer\HandlerRegistry*
    ignoreErrors:
        - '#Cannot call method rowCount\(\) on Doctrine\\DBAL\\Driver\\ResultStatement\|int.#'
    reportUnmatchedIgnoredErrors: false
