{
    "name": "ergonode/exporter",
    "type": "ergonode-module",
    "description": "Ergonode - Exporter",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
       "sort-packages": true
    },
    "require": {
        "php": "^7.4",

        "jms/serializer": "^3.4",
        "ramsey/uuid": "^3.8",
        "symfony/messenger": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Exporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Exporter\\Tests\\": "tests/"
        }
    }
}
