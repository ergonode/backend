{
  "name": "ergonode/exporter-file",
  "type": "ergonode-module",
  "description": "Ergonode - Exporter File",
  "homepage": "https://ergonode.com",
  "license": "OSL-3.0",
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "require": {
    "php": "^7.4",
    "ergonode/channel": "^1.0.0-rc.2",
    "league/flysystem": ">1.0.54",
    "league/flysystem-bundle": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Ergonode\\ExporterFile\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergonode\\ExporterFile\\Tests\\": "tests/"
    }
  }
}
