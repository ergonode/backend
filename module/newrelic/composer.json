{
    "name": "ergonode/newrelic",
    "type": "ergonode-module",
    "description": "Ergonode - New Relic",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "symfony/http-foundation": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "suggest": {
        "ext-newrelic": "*"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\NewRelic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\NewRelic\\Tests\\": "tests/"
        }
    }
}
