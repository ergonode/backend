{
    "name": "ergonode/core",
    "type": "ergonode-module",
    "description": "Ergonode - Core",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.9",
        "ergonode/account": "^0.6.0",
        "ergonode/api": "^0.6.0",
        "ergonode/grid": "^0.6.0",
        "ergonode/migration": "^0.6.0",
        "jms/serializer": "^3.1",
        "jms/serializer-bundle": "^3.1",
        "mhujer/jms-serializer-uuid": "^3.1",
        "moneyphp/money": "^3.2",
        "nelmio/api-doc-bundle": "^3.4",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.4",
        "symfony/form": "^4.4",
        "symfony/messenger": "^4.4",
        "symfony/translation": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Core\\Tests\\": "tests/"
        }
    }
}
