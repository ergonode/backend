{
    "name": "ergonode/notification",
    "type": "ergonode-module",
    "description": "Ergonode - Notification",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.10",
        "ergonode/account": "^1.0.0-beta.7",
        "ergonode/api": "^1.0.0-beta.7",
        "ergonode/core": "^1.0.0-beta.7",
        "ergonode/es": "^1.0.0-beta.7",
        "ergonode/grid": "^1.0.0-beta.7",
        "ergonode/migration": "^1.0.0-beta.7",
        "ergonode/shared-kernel": "^1.0.0-beta.7",
        "jms/serializer": "^3.4",
        "nelmio/api-doc-bundle": "^3.6",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/messenger": "^4.4",
        "symfony/translation": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Notification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Notification\\Tests\\": "tests/"
        }
    }
}
