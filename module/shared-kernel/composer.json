{
  "name": "ergonode/shared-kernel",
  "type": "ergonode-module",
  "description": "Ergonode - Shared Kernel",
  "homepage": "https://ergonode.com",
  "license": "OSL-3.0",
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "require": {
    "php": "^7.4",
    "egulias/email-validator": "^2.1",
    "ramsey/uuid": "^3.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Ergonode\\SharedKernel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergonode\\SharedKernel\\Tests\\": "tests/"
    }
  }
}
