{
    "name": "ergonode/generator",
    "type": "ergonode-module",
    "description": "Ergonode - Generator",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "doctrine/collections": "^1.6",
        "doctrine/dbal": "^2.10",
        "ergonode/core": "^1.0.0-rc.2",
        "ergonode/es": "^1.0.0-rc.2",
        "nette/php-generator": "^3.2",
        "symfony/http-foundation": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Generator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Generator\\Tests\\": "tests/"
        }
    }
}
