{
    "name": "ergonode/product-simple",
    "type": "ergonode-module",
    "description": "Ergonode - Product Simple",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "ergonode/designer": "^0.5.0",
        "ergonode/product": "^0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\ProductSimple\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\ProductSimple\\Tests\\": "tests/"
        }
    }
}
