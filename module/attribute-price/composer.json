{
    "name": "ergonode/attribute-price",
    "type": "ergonode-module",
    "description": "Ergonode - Attribute Price",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "doctrine/dbal": "^2.9",
        "ergonode/api": "^0.5.0",
        "ergonode/attribute": "^0.5.0",
        "ergonode/core": "^0.5.0",
        "ergonode/es": "^0.5.0",
        "jms/serializer": "^3.1",
        "moneyphp/money": "^3.2",
        "nelmio/api-doc-bundle": "^3.4",
        "symfony/form": "^4.3",
        "symfony/http-foundation": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\AttributePrice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\AttributePrice\\Tests\\": "tests/"
        }
    }
}
