{
  "name": "ergonode/exporter-magento-2",
  "type": "ergonode-module",
  "description": "Ergonode - Exporter Magento 2",
  "homepage": "https://ergonode.com",
  "license": "OSL-3.0",
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "require": {
    "php": "^7.4",
    "ergonode/channel": "^1.0.0-rc.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Ergonode\\ExporterMagento2\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergonode\\ExporterMagento2\\Tests\\": "tests/"
    }
  }
}
