{
    "name": "ergonode/translation-deepl",
    "type": "ergonode-module",
    "description": "Ergonode - Translation Deepl",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "doctrine/collections": "^1.6",
        "doctrine/dbal": "^2.10",
        "ergonode/api": "^1.0.0-rc.2",
        "ergonode/core": "^1.0.0-rc.2",
        "symfony/http-foundation": "^4.4",
        "scn/deepl-api-connector": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\TranslationDeepl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\TranslationDeepl\\Tests\\": "tests/"
        }
    }
}
