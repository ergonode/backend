{
    "name": "ergonode/comment",
    "type": "ergonode-module",
    "description": "Ergonode - Comment",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.10",
        "ergonode/account": "^0.8.0",
        "ergonode/api": "^0.8.0",
        "ergonode/core": "^0.8.0",
        "ergonode/es": "^0.8.0",
        "ergonode/grid": "^0.8.0",
        "ergonode/shared-kernel": "^0.8.0",
        "jms/serializer": "^3.4",
        "nelmio/api-doc-bundle": "^3.6",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/form": "^4.4",
        "symfony/messenger": "^4.4",
        "symfony/translation": "^4.4",
        "symfony/validator": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Comment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Comment\\Tests\\": "tests/"
        }
    }
}
