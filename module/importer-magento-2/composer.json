{
  "name": "ergonode/importer-magento-2",
  "type": "ergonode-module",
  "description": "Ergonode - Importer Magento 2",
  "homepage": "https://ergonode.com",
  "license": "OSL-3.0",
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "require": {
    "php": "^7.4",
    "ergonode/importer": "^0.10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Ergonode\\ImporterMagento2\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergonode\\ImporterMagento2\\Tests\\": "tests/"
    }
  }
}
