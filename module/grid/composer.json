{
    "name": "ergonode/grid",
    "type": "ergonode-module",
    "description": "Ergonode - Grid",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "doctrine/collections": "^1.6",
        "doctrine/dbal": "^2.13",
        "ergonode/attribute": "^1.0.0",
        "ergonode/core": "^1.0.0",
        "ergonode/shared-kernel": "^1.0.0",
        "symfony/http-foundation": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Grid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Grid\\Tests\\": "tests/"
        }
    }
}
