{
    "name": "ergonode/segment",
    "type": "ergonode-module",
    "description": "Ergonode - Segment",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.13",
        "ergonode/attribute": "^1.3.0",
        "ergonode/product": "^1.3.0",
        "ergonode/core": "^1.3.0",
        "ergonode/condition": "^1.3.0",
        "ergonode/es": "^1.3.0",
        "ergonode/grid": "^1.3.0",
        "ergonode/shared-kernel": "^1.3.0",
        "ergonode/value": "^1.3.0",
        "nelmio/api-doc-bundle": "^3.6",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/form": "^4.4.11|^5.3",
        "symfony/messenger": "^4.4|^5.3",
        "symfony/translation": "^4.4|^5.3",
        "symfony/validator": "^4.4.11|^5.3"
    },
    "require-dev": {
        "ergonode/completeness": "^1.3.0",
        "phpunit/phpunit": "^9"
    },
    "suggest": {
        "ergonode/completeness": "For using the module with completeness conditions"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Segment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Segment\\Tests\\": "tests/"
        }
    }
}
