{
    "name": "ergonode/fixture",
    "type": "ergonode-module",
    "description": "Ergonode - Fixture",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "ergonode/condition": "^0.5.0",
        "ergonode/es": "^0.5.0",
        "ergonode/workflow": "^0.5.0",
        "nelmio/alice": "^3.5",
        "symfony/config": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/http-kernel": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Fixture\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Fixture\\Tests\\": "tests/"
        }
    }
}
