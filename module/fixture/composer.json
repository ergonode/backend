{
    "name": "ergonode/fixture",
    "type": "ergonode-module",
    "description": "Ergonode - Fixture",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "ergonode/account": "^0.8.0",
        "ergonode/attribute": "^0.8.0",
        "ergonode/category": "^0.8.0",
        "ergonode/comment": "^0.8.0",
        "ergonode/condition": "^0.8.0",
        "ergonode/core": "^0.8.0",
        "ergonode/designer": "^0.8.0",
        "ergonode/es": "^0.8.0",
        "ergonode/product": "^0.8.0",
        "ergonode/segment": "^0.8.0",
        "ergonode/shared-kernel": "^0.8.0",
        "ergonode/workflow": "^0.8.0",
        "nelmio/alice": "^3.5",
        "symfony/config": "^4.4",
        "symfony/console": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Fixture\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Fixture\\Tests\\": "tests/"
        }
    }
}
