{
    "name": "ergonode/migration",
    "type": "ergonode-module",
    "description": "Ergonode - Migration",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "doctrine/migrations": "2.2.x",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Migration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Migration\\Tests\\": "tests/"
        }
    }
}
