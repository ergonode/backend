{
    "name": "ergonode/importer",
    "type": "ergonode-module",
    "description": "Ergonode - Importer",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.13",
        "ergonode/api": "^1.2.0",
        "ergonode/core": "^1.2.0",
        "ergonode/grid": "^1.2.0",
        "ergonode/reader": "^1.2.0",
        "ergonode/shared-kernel": "^1.2.0",
        "limenius/liform-bundle": "^0.17.0",
        "nelmio/api-doc-bundle": "^3.6",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/form": "^4.4.11|^5.3",
        "symfony/messenger": "^4.4|^5.3",
        "symfony/translation": "^4.4|^5.3",
        "symfony/validator": "^4.4.11|^5.3",
        "vich/uploader-bundle": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Importer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Importer\\Tests\\": "tests/"
        }
    }
}
