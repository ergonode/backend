{
    "name": "ergonode/es",
    "type": "ergonode-module",
    "description": "Ergonode - Event Sourcing",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.10",
        "ergonode/core": "^1.0.0-rc.2",
        "ergonode/shared-kernel": "^1.0.0-rc.2",
        "symfony/cache": "^4.4",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/security": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\EventSourcing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\EventSourcing\\Tests\\": "tests/"
        }
    }
}
