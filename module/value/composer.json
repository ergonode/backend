{
    "name": "ergonode/value",
    "type": "ergonode-module",
    "description": "Ergonode - Value",
    "homepage": "https://ergonode.com",
    "license": "OSL-3.0",
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.13",
        "ergonode/attribute": "^1.1.0",
        "ergonode/core": "^1.1.0",
        "ergonode/es": "^1.1.0",
        "ergonode/shared-kernel": "^1.1.0",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ergonode\\Value\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ergonode\\Value\\Tests\\": "tests/"
        }
    }
}
